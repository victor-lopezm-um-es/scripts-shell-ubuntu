[?2004l#include <string.h>
#include <unistd.h>
#include <stdlib.h>

#define MAX_SIZE 100

char buffer[MAX_SIZE];
char *msg1 = "Leyendo por stdin: ";
char *msg2 = "Escribiendo en stderr los datos leidos por stdin : ";
char *msg3 = "Ahora voy a ejecutar 'ls /'\n";

int main(void) {
    write(1,msg1, strlen(msg1));       // write msg1 to stdout
    read(0,buffer, MAX_SIZE);          // read from stdin
    write(1,msg2, strlen(msg2));       // write msg2 to stdout
    write(2,buffer, strlen(buffer));   // write data read from stdin to stderr
    write(1,msg3, strlen(msg3));       // write msg3 to stdout
    execl("/bin/ls", "ls", "/",NULL);  // execute program 'ls'
}
