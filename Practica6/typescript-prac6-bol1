lr-x------ 1 alumno alumno 64 nov 14 19:24 [01;36m3[0m -> /home/alumno/FicherosBoletinPractica6/input_openfiles.txt
lr-x------ 1 alumno alumno 64 nov 14 19:24 [01;36m4[0m -> /home/alumno/FicherosBoletinPractica6/input_openfiles.txt
lrwx------ 1 alumno alumno 64 nov 14 19:24 [01;36m5[0m -> /home/alumno/FicherosBoletinPractica6/output_openfiles.txt
[?2004h]0;alumno@alumno-VirtualBox: ~/FicherosBoletinPractica6[01;32malumno@alumno-VirtualBox[00m:[01;34m~/FicherosBoletinPractica6[00m$ cat openfiles.c
[?2004l#include <assert.h>
#include <ctype.h>
#include <stdio.h>
#include <stdlib.h>
#include <unistd.h>
#include <sys/stat.h>

const  char *input_filename = "input_openfiles.txt";
const  char *output_filename = "output_openfiles.txt";

int main (void) {
    // Abrir fichero de entrada
    FILE * filein_mitad = fopen (input_filename, "r");
    assert(filein_mitad != NULL);
    if (filein_mitad == NULL) {
        fprintf(stderr,"Error al abrir el fichero %s\n", input_filename);
        exit(-1);
    }
    // Abrir fichero de entrada nuevamente (diferente puntero)
    FILE * filein_inicio = fopen (input_filename, "r");
    assert(filein_inicio != NULL);

    // Crear/truncar fichero de salida
    FILE * fileout = fopen (output_filename, "w+");
    assert(fileout != NULL);

    // Obtener tama√±o del fichero
    struct stat st;
    stat(input_filename, &st);
    int size = st.st_size;

    // Mover puntero a la mitad del fichero
    int success = fseek(filein_mitad, size/2, SEEK_SET);
    assert(success == 0);
    long half_offset = ftell(filein_mitad);

    ssize_t bytes_leidos;
    size_t numero_bytes = 0;
    char *cadena = NULL;
    // Alternamos lectura en uno y otro fichero l√≥gico
    FILE *current = filein_inicio;
    FILE *previous = filein_mitad;
    // Leer l√≠nea a l√≠nea, alternando fichero de entrada
    for(int i=0; i < 20; ++i) {
        numero_bytes = 0;
        cadena = NULL; // B√∫fer reservado por getline
        // Leer datos del fichero de entrada "current"
        bytes_leidos = getline(&cadena, &numero_bytes, current);
        assert(bytes_leidos > 0);
        // Escribir datos en el fichero de salida
        fprintf(fileout,"%s", cadena);
        free(cadena);
        // Intercambiar fichero a leer: ini->fin->ini->fin
        FILE *aux = previous;
        previous = current;
        current = aux;
    }
    printf("Pulsa INTRO para continuar..");
    getchar();
    
    fclose(filein_mitad); 
    fclose(filein_inicio);
    fclose(fileout);
  
    return(0);
}
