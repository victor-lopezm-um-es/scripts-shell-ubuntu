#include <ctype.h>
#include <stdio.h>
#include <stdlib.h>
#include <unistd.h>

const  char *filename = "hello_fileio.txt";

int main (void) {
    FILE * fp;
    fprintf(stdout, "Creando fichero '%s' (ser√° truncado si existe)\n", filename);
    fp = fopen ("noexiste", "r");
    assert(fp == NULL);
    fp = fopen (filename, "w+");
    fprintf(fp, "¬°Hola, mundo!\n");
    fclose(fp);
   
    return(0);
}
